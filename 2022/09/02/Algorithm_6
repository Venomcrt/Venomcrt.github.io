pan><br><span class="line">		&#125;</span><br><span class="line">	&#125;</span><br><span class="line">&#125;</span><br><span class="line"></span><br><span class="line">void LCS(int i,int j,char*x,int**b) &#123;</span><br><span class="line">	if (i == 0 || j == 0) &#123;</span><br><span class="line">		return;</span><br><span class="line">	&#125;</span><br><span class="line">	if (b[i][j] == 1) &#123;</span><br><span class="line">		LCS(i - 1, j - 1, x,b);</span><br><span class="line">	cout &lt;&lt; x[i-1];</span><br><span class="line">	&#125;</span><br><span class="line">	else if (b[i][j] == 2) &#123;</span><br><span class="line">		LCS(i - 1, j, x, b);</span><br><span class="line">	&#125;</span><br><span class="line">	else &#123;</span><br><span class="line">		LCS(i, j - 1, x, b);</span><br><span class="line">	&#125;</span><br><span class="line">&#125;</span><br><span class="line">void p(int** t,int m,int n) &#123;</span><br><span class="line">	for (int i = 0; i &lt; m; i++) &#123;</span><br><span class="line">		for (int j = 0; j &lt; n; j++) &#123;</span><br><span class="line">			cout &lt;&lt; t[i][j];</span><br><span class="line">		&#125;</span><br><span class="line">		cout &lt;&lt; endl;</span><br><span class="line">	&#125;</span><br><span class="line">&#125;</span><br><span class="line">int main()</span><br><span class="line">&#123;</span><br><span class="line">	char* x=(char*)&quot;safasdkhgasdg&quot;;</span><br><span class="line">	char* y = (char*)&quot;adgasg&quot;;</span><br><span class="line">	int m, n;</span><br><span class="line">	m = strlen(x);</span><br><span class="line">	n = strlen(y);</span><br><span class="line">	int** c=new int*[m];</span><br><span class="line">	int** b = new int* [m];</span><br><span class="line">	for (int i = 0; i &lt;= m; i++)&#123;</span><br><span class="line">		c[i] = new int[n];</span><br><span class="line">	&#125;</span><br><span class="line">	for (int i = 0; i &lt;= m; i++) &#123;</span><br><span class="line">		b[i] = new int[n];</span><br><span class="line">	&#125;</span><br><span class="line">	LCSLength(m, n, x, y, c, b);</span><br><span class="line">	p(c, m, n);</span><br><span class="line">	cout &lt;&lt; endl;</span><br><span class="line">	p(b, m, n);</span><br><span class="line">	LCS(m,n,x,b);</span><br><span class="line"></span><br><span class="line">&#125;</span><br></pre></td></tr></table></figure>
